<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="92dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.cardview.widget.CardView
        android:id="@+id/item_profile_pre_activity_layout_cv"
        android:layout_width="match_parent"
        android:layout_height="79dp"
        app:cardCornerRadius="10dp"
        android:layout_marginHorizontal="8dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/item_profile_pre_activity_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.cardview.widget.CardView
                android:id="@+id/item_profile_pre_activity_layout_section_cv"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_marginTop="12dp"
                android:layout_marginStart="19.39dp"
                app:cardCornerRadius="26dp"
                app:cardElevation="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <ImageView
                    android:id="@+id/item_profile_pre_activity_layout_section_iv"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="#D9D9D9" />

            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/item_profile_pre_activity_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="한둘셋넷닷한둘셋넷닷총열하나"
                android:textColor="@color/top"
                android:fontFamily="@font/spoqa_medium"
                android:textFontWeight="700"
                android:textSize="13sp"
                android:maxLines="1"
                android:ellipsize="end"
                android:layout_marginStart="11dp"
                android:layout_marginEnd="18dp"
                app:layout_constraintStart_toEndOf="@id/item_profile_pre_activity_layout_section_cv"
                app:layout_constraintTop_toTopOf="@id/item_profile_pre_activity_layout_section_cv"
                app:layout_constraintBottom_toBottomOf="@id/item_profile_pre_activity_layout_section_cv"
                app:layout_constraintEnd_toEndOf="parent"/>

            <ImageView
                android:id="@+id/item_profile_pre_activity_member_iv"
                android:layout_width="13.33dp"
                android:layout_height="11.33dp"
                android:layout_marginStart="23.33dp"
                android:layout_marginBottom="10dp"
                android:src="@drawable/ic_member_gray"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <TextView
                android:id="@+id/item_profile_pre_activity_member_number_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="4"
                android:textColor="@color/gray_2"
                android:fontFamily="@font/spoqa_medium"
                android:textFontWeight="500"
                android:textSize="14sp"
                android:layout_marginStart="10.33dp"
                app:layout_constraintTop_toTopOf="@id/item_profile_pre_activity_member_iv"
                app:layout_constraintBottom_toBottomOf="@id/item_profile_pre_activity_member_iv"
                app:layout_constraintStart_toEndOf="@id/item_profile_pre_activity_member_iv" />

            <TextView
                android:id="@+id/item_profile_pre_activity_category_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="중식"
                android:textColor="@color/gray_2"
                android:fontFamily="@font/spoqa_medium"
                android:textFontWeight="500"
                android:textSize="13sp"
                android:layout_marginStart="80dp"
                app:layout_constraintTop_toTopOf="@id/item_profile_pre_activity_member_iv"
                app:layout_constraintBottom_toBottomOf="@id/item_profile_pre_activity_member_iv"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/item_profile_pre_activity_date_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2022.08.19"
                android:textColor="@color/gray_2"
                android:textSize="13sp"
                android:textFontWeight="500"
                android:fontFamily="@font/spoqa_medium"
                android:layout_marginEnd="23dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/item_profile_pre_activity_member_iv"
                app:layout_constraintBottom_toBottomOf="@id/item_profile_pre_activity_member_iv" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>